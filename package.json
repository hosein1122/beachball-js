{
  "name": "beachball",
  "version": "1.0.0",
  "description": "A full JavaScript/Node.js port of the `beachball.py` module from ObsPy to render earthquake focal mechanism diagrams (beachballs) on HTML canvas.",
  "type": "module",
  "sideEffects": false,
  "main": "dist/beachball.js",
  "directories": {
    "test": "jest"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "rollup -c"
  },
  "keywords": [
    "seismology",
    "beachball",
    "focal-mechanism",
    "canvas",
    "obspy",
    "earthquake"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^30.0.4",
    "rollup": "^2.79.2",
    "rollup-plugin-visualizer": "^6.0.3"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.1.1"
  }
}
